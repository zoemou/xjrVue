    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-vue": "^6.2.2",
    "js-md5": "^0.7.3",
    "node-sass": "^4.9.0",
    "sass": "^1.23.7",
    "sass-loader": "^9.0.3",
    "svg-sprite-loader": "^4.1.6",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
